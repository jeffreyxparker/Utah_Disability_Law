workflow_order <- c("PENDING FOLDER ASSEMBLY",
"FOLDER ASSEMBLY",
"PRE HEARING REVIEW",
"PRE HEARING DEVELOPMENT",
"READY TO SCHEDULE",
"SCHEDULED HEARING",
"POST HEARING REVIEW",
"POST HEARING DEVELOPMENT",
"PENDING DECISION WRITING",
"DECISION WRITING PROCESS",
"CASE CLOSED")
new_data$`Status of Case` <- factor(new_data$`Status of Case`,levels = workflow_order)
bar <- ggplot(data = new_data) +
geom_bar(mapping = aes(x = `Status of Case`)) +
coord_flip() +
labs(y = "Last File Received", x = NULL)
bar
